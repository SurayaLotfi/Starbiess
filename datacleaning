## for y2015_male
for (i in 1:nrow(y2015_male)) {
  for (j in 4:ncol(y2015_male)) {
    if(grepl(",",y2015_male[i,j], fixed = TRUE)){
      y2015_male[i,j] <- gsub(',','',as.character(y2015_male[i,j]))
      y2015_male[i,j]
    }
    if(!grepl("0.0",y2015_male[i,j], fixed = TRUE)){
      y2015_male[i,j] <- 1000*(as.numeric(y2015_male[i,j]))
    }
  }
}


## for y2015_female
for (i in 1:nrow(y2015_female)) {
  for (j in 4:ncol(y2015_female)) {
    if(grepl(",",y2015_female[i,j], fixed = TRUE)){
      y2015_female[i,j] <- gsub(',','',as.character(y2015_female[i,j]))
      y2015_female[i,j]
    }
    if(!grepl("0.0",y2015_female[i,j], fixed = TRUE)){
      y2015_female[i,j] <- 1000*(as.numeric(y2015_female[i,j]))
    }
  }
}


