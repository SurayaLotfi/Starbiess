# Group disease name under 1 column function

## for y2k_total
for (i in 1:(nrow(y2k_total))) {
  if(is.na(y2k_total[i,4]) | (grepl(".",y2k_total[i,4], fixed = TRUE))){
    y2k_total[i,4]
    grepl(".",y2k_total[i,4], fixed = TRUE)
    if(is.na(y2k_total[i,7])){
      if(is.na(y2k_total[i,6])){
        y2k_total[i,4] <- y2k_total[i,5]
      }
      else{
        y2k_total[i,4] <- y2k_total[i,6]
      }
    }
    else{
      y2k_total[i,4] <- y2k_total[i,7]
    }
  }
}
y2k_total <- y2k_total[,-c(3,5,6,7)]

## for y2k_male
for (i in 1:(nrow(y2k_male))) {
  if(is.na(y2k_male[i,4]) | (grepl(".",y2k_male[i,4], fixed = TRUE))){
