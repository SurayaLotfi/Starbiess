## for y2k_total
for(i in 4:6){
  for (j in 1:(nrow(y2k_total)-1)) {
    # if data has no '.' & is not empty
    if(!(grepl(".",y2k_total[j,i], fixed = TRUE)) & !(is.na(y2k_total[j,i]))){
      if((grepl(".",y2k_total[j+1,i], fixed = TRUE))){
        y2k_total <- y2k_total[-c(j),]
      }
    }
  }
}

## for y2k_male
for(i in 4:6){
  for (j in 1:(nrow(y2k_male)-1)) {
    
    # if data has no '.' & is not empty
    if(!(grepl(".",y2k_male[j,i], fixed = TRUE)) & !(is.na(y2k_male[j,i]))){
      if((grepl(".",y2k_male[j+1,i], fixed = TRUE))){
        y2k_male <- y2k_male[-c(j),]
        
      }
    }
  }
}

## for y2k_female
for(i in 4:6){
  for (j in 1:(nrow(y2k_female)-1)) {
    
    # if data has no '.' & is not empty
    if(!(grepl(".",y2k_female[j,i], fixed = TRUE)) & !(is.na(y2k_female[j,i]))){
      if((grepl(".",y2k_female[j+1,i], fixed = TRUE))){
        y2k_female <- y2k_female[-c(j),]
        
