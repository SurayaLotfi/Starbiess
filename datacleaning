# replace all instances of . to 0

y2k_total[y2k_total=="."] <- "0"
y2k_male[y2k_male=="."] <- "0"
y2k_female[y2k_female=="."] <- "0"
y2010_total[y2010_total=="."] <- "0"
y2010_male[y2010_male=="."] <- "0"
y2010_female[y2010_female=="."] <- "0"
y2015_total[y2015_total=="."] <- "0"
y2015_male[y2015_male=="."] <- "0"
y2015_female[y2015_female=="."] <- "0"
y2019_total[y2019_total=="."] <- "0"
y2019_male[y2019_male=="."] <- "0"
y2019_female[y2019_female=="."] <- "0"


# multiply all numbers by 1000

## for y2k_total
for (i in 1:nrow(y2k_total)) {
  for (j in 4:ncol(y2k_total)) {
    if(grepl(",",y2k_total[i,j], fixed = TRUE)){
      y2k_total[i,j] <- gsub(',','',as.character(y2k_total[i,j]))
      y2k_total[i,j]
    }
    if(!grepl("0.0",y2k_total[i,j], fixed = TRUE)){
      y2k_total[i,j] <- 1000*(as.numeric(y2k_total[i,j]))
    }
  }
}

## for y2k_male
for (i in 1:nrow(y2k_male)) {
  for (j in 4:ncol(y2k_male)) {
    if(grepl(",",y2k_male[i,j], fixed = TRUE)){
      y2k_male[i,j] <- gsub(',','',as.character(y2k_male[i,j]))
      y2k_male[i,j]
    }
    if(!grepl("0.0",y2k_male[i,j], fixed = TRUE)){
      y2k_male[i,j] <- 1000*(as.numeric(y2k_male[i,j]))
    }
  }
}


## for y2k_female
for (i in 1:nrow(y2k_female)) {
  for (j in 4:ncol(y2k_female)) {
