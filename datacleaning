    }
  }
}


# replace all instances of 0.0 to less than 1000

y2k_total[y2k_total=="0.0"] <- "less than 1000"
y2k_male[y2k_male=="0.0"] <- "less than 1000"
y2k_female[y2k_female=="0.0"] <- "less than 1000"
