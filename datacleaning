# put data into data frame

y2k <- read.csv("Mortality by country y2k.csv",na.strings = c("","NA"))
y2010 <- read.csv("Mortality by country y2010.csv",na.strings = c("","NA"))
y2015 <- read.csv("Mortality by country y2015.csv",na.strings = c("","NA"))
y2019 <- read.csv("Mortality by country y2019.csv",na.strings = c("","NA"))
y2k_df <- data.frame(y2k)
y2010_df <- data.frame(y2010)
y2015_df <- data.frame(y2015)
y2019_df <- data.frame(y2019)


# remove rows

y2k_df <- y2k_df[-(1:5),]
y2010_df <- y2010_df[-(1:5),]
y2015_df <- y2015_df[-(1:5),]
y2019_df <- y2019_df[-(1:5),]


# set header

names(y2k_df) <- y2k_df[1,]
y2k_df <- y2k_df[-(1:3),]

names(y2010_df) <- y2010_df[1,]
y2010_df <- y2010_df[-(1:3),]

names(y2015_df) <- y2015_df[1,]
y2015_df <- y2015_df[-(1:3),]
names(y2019_df) <- y2019_df[1,]
y2019_df <- y2019_df[-(1:3),]


#remove extra columns

y2k_df <- y2k_df[,-which(names(y2k_df) %in% ("NA"))]
y2010_df <- y2010_df[,-which(names(y2010_df) %in% ("NA"))]
y2015_df <- y2015_df[,-which(names(y2015_df) %in% ("NA"))]
y2019_df <- y2019_df[,-which(names(y2019_df) %in% ("NA"))]


# split male female total

y2k_total <- y2k_df[is.element(y2k_df$Sex,'Persons'),]
y2k_male <- y2k_df[is.element(y2k_df$Sex,'Males'),]
y2k_female <- y2k_df[is.element(y2k_df$Sex,'Females'),]

y2010_total <- y2010_df[is.element(y2010_df$Sex,'Persons'),]
y2010_male <- y2010_df[is.element(y2010_df$Sex,'Males'),]
y2010_female <- y2010_df[is.element(y2010_df$Sex,'Females'),]

y2015_total <- y2015_df[is.element(y2015_df$Sex,'Persons'),]
y2015_male <- y2015_df[is.element(y2015_df$Sex,'Males'),]
y2015_female <- y2015_df[is.element(y2015_df$Sex,'Females'),]

y2019_total <- y2019_df[is.element(y2019_df$Sex,'Persons'),]
y2019_male <- y2019_df[is.element(y2019_df$Sex,'Males'),]
y2019_female <- y2019_df[is.element(y2019_df$Sex,'Females'),]


# remove injuries

y2k_total <- y2k_total[-((which(y2k_total$`GHE cause`=='Injuries')):nrow(y2k_total)),]
y2010_total <- y2010_total[-((which(y2010_total$`GHE cause`=='Injuries')):nrow(y2010_total)),]
y2015_total <- y2015_total[-((which(y2015_total$`GHE cause`=='Injuries')):nrow(y2015_total)),]
y2019_total <- y2019_total[-((which(y2019_total$`GHE cause`=='Injuries')):nrow(y2019_total)),]

y2k_male <- y2k_male[-((which(y2k_male$`GHE cause`=='Injuries')):nrow(y2k_male)),]
y2010_male <- y2010_male[-((which(y2010_male$`GHE cause`=='Injuries')):nrow(y2010_male)),]
y2015_male <- y2015_male[-((which(y2015_male$`GHE cause`=='Injuries')):nrow(y2015_male)),]
y2019_male <- y2019_male[-((which(y2019_male$`GHE cause`=='Injuries')):nrow(y2019_male)),]

y2k_female <- y2k_female[-((which(y2k_female$`GHE cause`=='Injuries')):nrow(y2k_female)),]
y2010_female <- y2010_female[-((which(y2010_female$`GHE cause`=='Injuries')):nrow(y2010_female)),]
y2015_female <- y2015_female[-((which(y2015_female$`GHE cause`=='Injuries')):nrow(y2015_female)),]
y2019_female <- y2019_female[-((which(y2019_female$`GHE cause`=='Injuries')):nrow(y2019_female)),]


# Remove unnecessary rows

